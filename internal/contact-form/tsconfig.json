{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@namekit/tsconfig/base.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "Preserve", // it was a different one
    "moduleResolution": "node", // it was a different one
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"]
  },
  
  // "include": ["src/**/*"]
}