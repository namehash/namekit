      "import": "./dist/index.css",
      "default": "./dist/index.css"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "styles": "./dist/index.css",
  "dependencies": {
    "@headlessui-float/react": "0.11.4",
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.0.18",
    "@namehash/ens-utils": "workspace:*",
    "@namehash/ens-webfont": "workspace:*",
    "@namehash/nameguard": "workspace:*",
    "classcat": "5.0.4",
    "sonner": "1.2.3",
    "swr": "2.2.4",
    "zustand": "4.4.5"
  },
  "devDependencies": {
    "@storybook/react": "8.1.8",
    "@storybook/test": "^8.1.8",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "postcss": "8.4.38",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
